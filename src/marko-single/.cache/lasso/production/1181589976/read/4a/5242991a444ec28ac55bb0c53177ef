$rmod.def("/main", function(require, exports, module, __filename, __dirname) { var template = require('/$/marko'/*'marko'*/)
    .load(require.resolve('./template.marko'));

template.render(
    {
        marko: 'marko'
    },
    function(err, html) {
        console.log('Template output: ' + html);
    });
});